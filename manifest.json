{
    "manifest_version": 3,
    "name": "Font Detector",
    "version": "1.0.0",
    "description": "Detects and displays all font combinations found on a webpage",
    "permissions": ["activeTab", "scripting"],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
